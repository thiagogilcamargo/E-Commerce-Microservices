    "esModuleInterop": true,                        /* Habilita a interoperabilidade de emissão entre CommonJS e Módulos ES via criação de objetos de namespace para todas as importações. Implica 'allowSyntheticDefaultImports'. */
    // "preserveSymlinks": true,                    /* Não resolve o caminho real dos links simbólicos. */
    // "allowUmdGlobalAccess": true,                /* Permite o acesso a globais UMD a partir de módulos. */

    /* Opções de Mapa de Origem */
    // "sourceRoot": "",                            /* Especifique o local onde o depurador deve localizar arquivos TypeScript em vez de locais de origem. */
    // "mapRoot": "",                               /* Especifique o local onde o depurador deve localizar arquivos de mapa em vez de locais gerados. */
    // "inlineSourceMap": true,                     /* Emitir um único arquivo com mapas de origem em vez de ter um arquivo separado. */
