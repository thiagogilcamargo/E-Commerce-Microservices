    // "noEmit": true,                              /* Não emite saídas. */
    // "importHelpers": true,                       /* Importa ajudantes de emissão de 'tslib'. */
    // "downlevelIteration": true,                  /* Fornece suporte completo para iteráveis em 'for-of', spread e destructuring ao segmentar 'ES5' ou 'ES3'. */
    // "isolatedModules": true,                     /* Transpila cada arquivo como um módulo separado (semelhante a 'ts.transpileModule'). */

    /* Opções de Verificação de Tipo Estrito */
    "strict": true,                                 /* Habilita todas as opções de verificação de tipo estrito. */
