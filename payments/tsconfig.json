    /* Opções Experimentais */
    // "experimentalDecorators": true,              /* Habilita suporte experimental para decoradores ES7. */
    // "emitDecoratorMetadata": true,               /* Habilita suporte experimental para emitir metadados de tipo para decoradores. */

    /* Opções Avançadas */
    "skipLibCheck": true,                           /* Ignora a verificação de tipo de arquivos de declaração. */
    "forceConsistentCasingInFileNames": true        /* Proíbe referências com letras maiúsculas inconsistentes para o mesmo arquivo. */
